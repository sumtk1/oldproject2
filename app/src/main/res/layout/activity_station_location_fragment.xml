<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_station_location_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gloiot.hygooilstation.ui.map.StationLocationFragmentActivity">

    <include
        android:id="@+id/in_titlebar"
        layout="@layout/include_titlebar" />

    <fragment
        android:id="@+id/map"
        class="com.amap.api.maps2d.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/in_titlebar" />

    <TextView
        android:id="@+id/tv_stationLocation_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="15dp"
        android:text="深圳市罗湖区"
        android:textColor="@color/orange_FF690C"
        android:textSize="28px" />


</com.zhy.autolayout.AutoRelativeLayout>
